    "vite-plugin-html": "^3.2.2",
    "vue": "^3.4.0",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.0",
    "cypress": "^13.6.0",
    "eslint": "^8.49.0",
